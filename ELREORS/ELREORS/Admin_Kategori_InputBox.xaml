<Window x:Class="ELREORS.Admin_Kategori_InputBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ELREORS"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="Admin_Kategori_InputBox" Height="200" Width="410">
    <Grid>
        <TextBox x:Name="textbox1"  Height="35" FontSize="18" Margin="0,30,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" VerticalContentAlignment="Center" HorizontalAlignment="Center" Width="370"/>
        <Button Content="Confirm" FontSize="16" VerticalAlignment="Top" Width="100" Height="33" Margin="0,105,130,0"  HorizontalAlignment="Right" Click="Button_Click"/>
            <Button Content="Cancel" FontSize="16" VerticalAlignment="Top" Width="100" Height="33" Margin="0,105,15,0" HorizontalAlignment="Right" Click="Button1_Click"/>
    </Grid>
</Window>
